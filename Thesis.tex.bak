\newcommand{\lbsum}[1]{
	\node(#1)[sum]{};
 	\draw [draw=GMVred]	(#1.north east) -- (#1.south west)
    		(#1.north west) -- (#1.south east);
	\draw [draw=GMVred]	(#1.north east) -- (#1.south west)
			(#1.north west) -- (#1.south east);
 
	\node[left=-1pt] at (#1.center){\textcolor{GMVred}{\scriptsize $\bm +$}};
	\node[below]	 at (#1.center){\textcolor{GMVred}{\scriptsize $\bm + $}};
	}

\newcommand{\pmtbsum}[1]{
	\node(#1)[sum]{};
 	\draw 	[draw=GMVred](#1.north east) -- (#1.south west)
    		(#1.north west) -- (#1.south east);
	\draw 	[draw=GMVred](#1.north east) -- (#1.south west)
			(#1.north west) -- (#1.south east);
 
	\node[above] at (#1.center){\textcolor{GMVred}{\scriptsize $\bm + $}};
	\node[below] at (#1.center){\textcolor{GMVred}{\scriptsize $\bm -$}};}

